body{font-family:Verdana,Arial,Helvetica,sans-serif;font-size:12px;}
div#header{background-color:rgb(57,65,89);color:#fff;font-family:Arial,Helvetica,sans-serif;font-weight:bold;font-size:12px;border:1px solid #000033;}
div#nav{font-family:Arial,Helvetica,sans-serif;font-size:12px;background:#EEE;border:1px solid #000033;color:#000;height:20px;} 
div#nav ul{margin:0px;padding:0px;} 
div#nav li{position:relative;list-style:none;margin:0px;float:left;font-weight:bold;line-height:1.2em;color:#000033;border-right:1px solid #ccc;padding-right:5px;padding-left:5px;} 
div#nav li:hover{color:#FFF;background:#D9D9D9;}
div#nav li a{display:block;padding:0.25em 0 0.25em 0.5em;text-decoration:none;color:#000033;font-size:10px;}
div#weather{font-family:Arial,Helvetica,sans-serif;}
div#weather a{color:#006600;}
h1,h2,h3
{
font-family: arial, 'sans serif'; 
}

p,table,li,address
{
font-family: arial, 'sans serif'; 
margin-left: 15pt;
}

li,th
{
font-size: 100%;
color: blue;
}

th {background-color:#FAEBD7}

body
{
background-color: #ffffff;
}

h1,h2,h3,hr 
{
color:saddlebrown; 
}

a:link    {color:darkred}
a:visited {color:darkred}
a:active  {color:red}

h1 {font-size: 14pt}
h2 {font-size: 12pt}
h3 {font-size: 11pt}
p, li {font-size: 10pt}

.table-font
{ font-size: 10pt; font-family: Courier;}
